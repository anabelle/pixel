# Pixel V2 Environment Variables
# Copy to ../.env and fill in values (shared with V1)

# AI Provider
AI_PROVIDER=openrouter
AI_MODEL=google/gemini-2.5-flash
OPENROUTER_API_KEY=
GOOGLE_API_KEY=
ANTHROPIC_API_KEY=

# Database
DATABASE_URL=postgresql://pixel:pixel@postgres-v2:5432/pixel_v2

# Server
PORT=4000
NODE_ENV=production

# Nostr Identity (NEVER expose, NEVER change)
NOSTR_PRIVATE_KEY=
NOSTR_PUBLIC_KEY=

# Lightning (NWC/Alby)
NWC_URL=

# Telegram
TELEGRAM_BOT_TOKEN=

